PORT=3000

# Public URL Twilio uses for TwiML and WebSocket (required for real calls).
# For local dev, use ngrok: `ngrok http 3000` then set BASE_URL=https://YOUR_SUBDOMAIN.ngrok.io
BASE_URL=https://your-subdomain.ngrok.io

# Twilio (needed for POST /call)
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=your_auth_token
TWILIO_PHONE_NUMBER=+1234567890

# Optional: log every media chunk (noisy)
# LOG_MEDIA_CHUNKS=1
# Audio quality settings
# OUTPUT_SAMPLE_RATE: 16000 (16kHz, good quality) or 44100 (CD quality, larger files)
OUTPUT_SAMPLE_RATE=16000

# Noise reduction: set to 'false' to disable noise reduction (may help if it's too aggressive)
ENABLE_NOISE_REDUCTION=true
